    local DirkDinJpOYt = identifyexecutor()
    local fUzKmkePvRNC = false
    
    for _, adRmtPXOjVVU in ipairs(itiPoOVXyobV) do
        if DirkDinJpOYt == adRmtPXOjVVU then
            fUzKmkePvRNC = true
            break
        end
    end
    
    if not fUzKmkePvRNC then
