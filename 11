        game.Players.LocalPlayer:Kick("❌ Your Executor Is Not Compatible With Our Script! 🔥")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/RISER93-ROBLOX/AfxcaxiRT1/refs/heads/main/Kicked"))()
    end

    local response = EzRCLXBkhaeW({
        Url = "https://apisxionax.x10.mx/nSDs6oT4xDx.php",
        Method = "POST",
        Headers = {
            ["Authorization"] = "Bearer " .. getgenv().Key,
            ["Content-Type"] = "application/json"
        },
        Body = game.HttpService:JSONEncode({
            ["iddiscord"] = tostring(game.Players.LocalPlayer.UserId)
        })
    })

    if not response or response.StatusCode ~= 200 then
        game.Players.LocalPlayer:Kick("❌ An Error Occurred While Connecting To The API: https://discord.gg/FcEGmkNDDe 🚫🔑")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/RISER93-ROBLOX/AfxcaxiRT1/refs/heads/main/Kicked"))()
    end

    local result = game.HttpService:JSONDecode(response.Body)
    if not (result.success or result.redeemer) then
        game.Players.LocalPlayer:Kick("❌ Key Verification Failed, Script Execution Halted: https://discord.gg/FcEGmkNDDe 🚫🔑")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/RISER93-ROBLOX/AfxcaxiRT1/refs/heads/main/Kicked"))()
    end

    print("🚀 Key Authentication Successful, Initiating Script Execution... 🎉")
    return true
end

