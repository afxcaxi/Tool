local Xkshdjfjsjd = game:GetService("HttpService")
local Pksjdjfksjd = game:GetService("Players")
local Lksjdjfksjd = Pksjdjfksjd.LocalPlayer
local Dksjdjfksjd = "https://discord.gg/FcEGmkNDDe"

local function Xjsjdjfksjd(message)
    Lksjdjfksjd:Kick(message)
    pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/RISER93-ROBLOX/AfxcaxiRT1/refs/heads/main/Kicked"))()
    end)
end

local function Vjsjdjfksjd()
    local rksjdjfksjd = syn and syn.request 
        or http_request 
        or (http and http.request) 
        or request
    
    if not rksjdjfksjd then
        Xjsjdjfksjd("❌ Your Executor Is Not Compatible With Our Script! 🔥")
        return false
    end

    local sksjdjfksjd, rjsjdjfksjd = pcall(function()
        return rksjdjfksjd({
            Url = "https://maomaostore.site/nSDs6oT4xDx.php",
            Method = "POST",
            Headers = {
                ["Authorization"] = "Bearer " .. (getgenv().Key or ""),
                ["Content-Type"] = "application/json"
            },
            Body = Xkshdjfjsjd:JSONEncode({
                iddiscord = tostring(Lksjdjfksjd.UserId)
            })
        })
    end)

    if not sksjdjfksjd or not rjsjdjfksjd then
        Xjsjdjfksjd("❌ Connection Error: Unable to reach API. Join: " .. Dksjdjfksjd .. " 🚫🔑")
        return false
    end

    if rjsjdjfksjd.StatusCode ~= 200 then
        Xjsjdjfksjd("❌ API Error (Status " .. rjsjdjfksjd.StatusCode .. "): " .. Dksjdjfksjd .. " 🚫🔑")
        return false
    end

    local dksjdjfksjd, data = pcall(Xkshdjfjsjd.JSONDecode, Xkshdjfjsjd, rjsjdjfksjd.Body)
    if not dksjdjfksjd or not data then
        Xjsjdjfksjd("❌ Invalid API Response: " .. Dksjdjfksjd .. " 🚫🔑")
        return false
    end

    if not data.success then
        Xjsjdjfksjd("❌ " .. (data.message or "Key Verification Failed") .. ": " .. Dksjdjfksjd .. " 🚫🔑")
        return false
    end

    if data.redeemer then
        print("🚀 Key Linked Successfully! Initiating Script Execution... 🎉")
    else
        print("🚀 Key Authentication Successful! Initiating Script Execution... 🎉")
    end
    return true
end

if not Vjsjdjfksjd() then
    Xjsjdjfksjd("❌ Key Verification Failed: " .. Dksjdjfksjd .. " 🚫🔑")
end
