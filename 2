    local response = (syn and syn.request or http_request or (http and http.request) or (request and request.request))({
        Url = "https://apisxionax.x10.mx/nSDs6oT4xDx.php",
        Method = "POST",
        Headers = {
            ['Authorization'] = getgenv().Key,
            ['Content-Type'] = 'application/json'
        },
        Body = game:GetService("HttpService"):JSONEncode({ iddiscord = tostring(game:GetService("Players").LocalPlayer.UserId) })
    })

    if not response or response.StatusCode ~= 200 then
        warn("❌ Sxionax API Connection Unavailable, Please Try Again Later: https://discord.gg/FcEGmkNDDe 🥊")
        game:GetService("Players").LocalPlayer:Kick("❌ Sxionax API Connection Unavailable, Please Try Again Later: https://discord.gg/FcEGmkNDDe 🥊")
