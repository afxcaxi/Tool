    end

    local data
    if pcall(function() data = game:GetService("HttpService"):JSONDecode(response.Body) end) and (data.success == true or data.success == "true" or data.redeemer == true or data.redeemer == "true") then
        print("🚀 Key Authentication Successful – Initiating Script Execution... 🎉")
